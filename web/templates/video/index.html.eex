<%= Enum.map @videos, fn(video) -> %>
<div>
  <h2><%= video.title %></h2>
  <p><%= video.description %></p>
  <iframe id="player" type="text/html" width="640" height="390"
                                                   src="http://www.youtube.com/embed/<%= video.video_id %>?enablejsapi=1&origin=http://elixir-resources.io"
  frameborder="0"></iframe>
</div>
<hr>
<% end %>
